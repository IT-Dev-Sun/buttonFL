o from \"@theatre/studio\"\nstudio.initialize()\n\nconst proj = getProject(\"foobarloremipsum\", {state})\nconst sheet = proj.sheet(\"Bouncing box\")\n\nconst nudgableNumber = t.number(1, {nudgeMultiplier: 0.01})\n\nfunction makeBouncingBox(i: number) {\n  const component = document.getElementById(\"component\")\n  // const boxContainer = document.getElementsByClassName(\"boxContainer\")[0]\n  const boxDiv = document.getElementsByClassName(\"box\")[0]\n  const dustDiv = document.getElementsByClassName(\"dust\")[0]\n  const textDiv = document.getElementsByClassName(\"text\")[0]\n  const sheet = proj.sheet(\"Bouncing box\")\n  \n  component.addEventListener(\"click\", () => {\n    sheet.sequence.play({iterationCount: 3});\n    component.style.backgroundColor = \"#ff00f7\"\n\n    })\n\n  const boxObj = sheet.object(\"Box\", {\n    y: 0,\n    stretch: nudgableNumber,\n  })\n\n  boxObj.onValuesChange(({y, stretch}) => {\n    boxDiv.style.transform = `translateY(${-y}px) scale(${\n      1 / stretch\n    }, ${stretch})`\n  })\n\n  const dustObj = sheet.object(\"Dust\", {\n    opacity: nudgableNumber,\n    scaleX: nudgableNumber,\n  })\n\n  dustObj.onValuesChange(({opacity, scaleX}) => {\n    dustDiv.style.opacity = String(opacity)\n    dustDiv.style.transform = `scaleX(${scaleX})`\n  })\n\n  const textObj = sheet.object(\"Text\", {\n    x: 10,\n    y: 10,\n  })\n\n  textObj.onValuesChange(({mouseover, click}) => {\n    textDiv.label = t.string('foo'),\n    textDiv.labelmouseover = t.string('bar'),\n    textDiv.labelclick = t.string('baz')\n  });\n}\n\nmakeBouncingBox(1)\n\n"},"lineCount":null}},"hash":"8c232b796812522db869193f49cfc2dd","cacheData":{"env":{}}}